<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragment">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        android:src="@drawable/header"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/one_logo"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/logo_olive"
        app:layout_constraintBottom_toTopOf="@+id/two_logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/five_logo"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/logo_canete"
        app:layout_constraintBottom_toTopOf="@+id/six_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/four_logo" />

    <ImageView
        android:id="@+id/six_logo"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/six_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/five_logo" />

    <ImageView
        android:id="@+id/four_logo"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/logo_bcnmilano"
        app:layout_constraintBottom_toTopOf="@+id/five_logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/three_logo" />

    <ImageView
        android:id="@+id/three_logo"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/logo_paco"
        app:layout_constraintBottom_toTopOf="@+id/four_logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/two_logo" />

    <ImageView
        android:id="@+id/two_logo"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/logo_barceloneta"
        app:layout_constraintBottom_toTopOf="@+id/three_logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/one_logo" />

    <ImageView
        android:id="@+id/one_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/img2"
        app:layout_constraintBottom_toBottomOf="@+id/one_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/one_logo"
        app:layout_constraintTop_toTopOf="@+id/one_logo"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/two_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/img3"
        app:layout_constraintBottom_toBottomOf="@+id/two_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/one_logo"
        app:layout_constraintTop_toTopOf="@+id/two_logo"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/three_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/img4"
        app:layout_constraintBottom_toBottomOf="@+id/three_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/one_logo"
        app:layout_constraintTop_toTopOf="@+id/three_logo"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/four_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/img1"
        app:layout_constraintBottom_toBottomOf="@+id/four_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/one_logo"
        app:layout_constraintTop_toTopOf="@+id/four_logo"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/five_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/img5"
        app:layout_constraintBottom_toBottomOf="@+id/five_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/one_logo"
        app:layout_constraintTop_toTopOf="@+id/five_logo"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/six_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/food_home"
        app:layout_constraintBottom_toBottomOf="@+id/six_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/one_logo"
        app:layout_constraintTop_toTopOf="@+id/six_logo"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="150dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/overlay"
        app:layout_constraintBottom_toBottomOf="@+id/five_image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/one_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:src="@drawable/ic_next"
        app:layout_constraintBottom_toBottomOf="@+id/one_image"
        app:layout_constraintEnd_toEndOf="@+id/one_image"
        app:layout_constraintTop_toTopOf="@+id/one_image"
        app:tint="@color/white" />

    <ImageView
        android:id="@+id/two_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:src="@drawable/ic_next"
        app:layout_constraintBottom_toBottomOf="@+id/two_image"
        app:layout_constraintEnd_toEndOf="@+id/one_image"
        app:layout_constraintTop_toBottomOf="@+id/one_image"
        app:tint="@color/white" />

    <ImageView
        android:id="@+id/three_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:src="@drawable/ic_next"
        app:layout_constraintBottom_toBottomOf="@+id/three_image"
        app:layout_constraintEnd_toEndOf="@+id/one_image"
        app:layout_constraintTop_toBottomOf="@+id/two_image"
        app:tint="@color/white" />

    <ImageView
        android:id="@+id/four_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:src="@drawable/ic_next"
        app:layout_constraintBottom_toBottomOf="@+id/four_image"
        app:layout_constraintEnd_toEndOf="@+id/one_image"
        app:layout_constraintTop_toBottomOf="@+id/three_image"
        app:tint="@color/white" />

    <ImageView
        android:id="@+id/five_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:src="@drawable/ic_next"
        app:layout_constraintBottom_toBottomOf="@+id/five_image"
        app:layout_constraintEnd_toEndOf="@+id/one_image"
        app:layout_constraintTop_toBottomOf="@+id/four_image"
        app:tint="@color/white" />

    <ImageView
        android:id="@+id/six_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:src="@drawable/ic_next"
        app:layout_constraintBottom_toBottomOf="@+id/six_image"
        app:layout_constraintEnd_toEndOf="@+id/one_image"
        app:layout_constraintTop_toBottomOf="@+id/five_image"
        app:tint="@color/white" />


</androidx.constraintlayout.widget.ConstraintLayout>